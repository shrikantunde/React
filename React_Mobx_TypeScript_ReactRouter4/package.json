{
  "name": "demo",
  "version": "1.0.0",
  "description": "React+Mobx+Typescript POC",
  "main": "webpack.config.js",
  "author": "Harman Team>",
  "license": "Harman",
  "scripts": {
    "start": "webpack-dev-server --inline --content-base . --history-api-fallback --port 3000",
    "build-dev": "rimraf ./dist && webpack-dev-server --config webpack.dev.config.js",
    "build": "rimraf ./dist && webpack --config webpack.build.js",
    "dev": "webpack -d --watch",
    "start2": "webpack-dev-server --watch",
    "prod": "npm run clean && webpack -p",
    "clean": "rimraf ./dist/*",
    "test": "jest"
  },
  "keywords": [
    "react",
    "react-router",
    "mobx",
    "mobx-react",
    "typescript",
    "webpack"
  ],
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bluebird": "^3.5.2",
    "@types/jest": "^19.2.3",
    "@types/react": "^15.0.20",
    "@types/react-dom": "^0.14.23",
    "@types/react-router-dom": "^4.0.1",
    "autoprefixer": "^6.7.7",
    "classnames": "^2.2.5",
    "css-loader": "^0.27.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "http-server": "^0.9.0",
    "image-webpack-loader": "^3.3.0",
    "jest": "^20.0.4",
    "node-sass": "^4.5.1",
    "postcss-loader": "^1.3.3",
    "precss": "^1.4.0",
    "react-d3-basic": "^1.6.11",
    "react-dnd": "^2.4.0",
    "react-dnd-html5-backend": "^2.4.1",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-test-renderer": "^15.5.4",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.3",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.16.0",
    "ts-jest": "^20.0.4",
    "ts-loader": "^2.0.3",
    "tslint": "^5.3.2",
    "tslint-loader": "^3.5.3",
    "twitter": "^1.7.1",
    "typescript": "^2.3.3",
    "url-loader": "^0.5.8",
    "validatorjs": "^3.13.3",
    "webpack": "^2.3.2",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "bluebird": "^3.5.0",
    "code-split-component": "2.0.0-alpha.5",
    "mobx": "^3.1.7",
    "mobx-react": "^4.1.3",
    "mobx-react-devtools": "^4.2.11",
    "moment": "^2.5.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "3.0.0",
    "react-router-dom": "^4.0.0",
    "whatwg-fetch": "^2.0.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
